{
  "definitions": {},
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "title": "The Data Schema",
      "items": {
        "$id": "#/properties/data/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "active",
          "created_at",
          "updated_at",
          "id",
          "name"
        ],
        "properties": {
          "active": {
            "$id": "#/properties/data/items/properties/active",
            "type": "boolean",
            "title": "The Active Schema",
            "default": false,
            "examples": [
              true
            ]
          },
          "created_at": {
            "$id": "#/properties/data/items/properties/created_at",
            "type": "string",
            "title": "The Created_at Schema",
            "default": "",
            "examples": [
              "2019-10-21T22:58:16.520Z"
            ],
            "pattern": "^(.*)$"
          },
          "updated_at": {
            "$id": "#/properties/data/items/properties/updated_at",
            "type": "string",
            "title": "The Updated_at Schema",
            "default": "",
            "examples": [
              "2019-10-21T22:58:16.520Z"
            ],
            "pattern": "^(.*)$"
          },
          "id": {
            "$id": "#/properties/data/items/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "name": {
            "$id": "#/properties/data/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "string"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}